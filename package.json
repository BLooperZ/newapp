{
  "name": "newapp",
  "version": "0.0.0",
  "description": "This is a work-in-progress gjs app of yet unknown purpose.",
  "main": "app.js",
  "scripts": {
    "build": "npm run build:shebang && npm run build:rollup && npm run build:permissions",
    "build:shebang": "echo '#!/usr/bin/env gjs\n' > bin/app",
    "build:rollup": "rollup -c >> bin/app",
    "build:permissions": "chmod +x bin/app"
  },
  "keywords": [
    "app",
    "node",
    "gjs"
  ],
  "author": "Niv Baehr",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1"
  }
}
